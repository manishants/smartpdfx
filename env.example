# Superadmin access is Supabase-only; ensure your profile has role "superadmin".
SUPERADMIN_EMAIL=admin@example.com
SUPERADMIN_PASSWORD=changeme-password
# Optional second credential pair (historical; not used when Supabase-only)
SUPERADMIN_EMAIL_2=owner@example.com
SUPERADMIN_PASSWORD_2=another-password

# Supabase: URL and public anonymous key (required)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-public-key

# Optional: Service Role key (server-only, NEVER expose to browser)
# SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Google Service Account credentials (preferred for server-side GA4)
GOOGLE_SERVICE_ACCOUNT_EMAIL=service-account@project.iam.gserviceaccount.com
GOOGLE_SERVICE_ACCOUNT_KEY="-----BEGIN PRIVATE KEY-----\nABC...\n-----END PRIVATE KEY-----\n"

# GA4 property
GA4_PROPERTY_ID=123456789

# Google Search Console site URL
GSC_SITE_URL=https://www.example.com/

# Choose auth method for GSC: "oauth" (recommended) or "service"
GSC_AUTH_METHOD=oauth

# GSC OAuth (use a refresh token generated via OAuth2)
GSC_OAUTH_CLIENT_ID=your-oauth-client-id
GSC_OAUTH_CLIENT_SECRET=your-oauth-client-secret
GSC_OAUTH_REDIRECT_URI=http://localhost:3000/api/gsc/oauth/callback
GSC_REFRESH_TOKEN=your-refresh-token

# Admin API key removed. Do not set client-side admin keys.

# Tabula Java jar path for PDFâ†’Excel No OCR (set to absolute path)
# Example (Windows): C:\\Program Files\\Tabula\\tabula.jar
# Example (Linux/Mac): /usr/local/share/tabula/tabula.jar
TABULA_JAR_PATH=